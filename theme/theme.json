{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": [
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "spacingSizes": [
        {
          "slug": "20",
          "size": "calc(var(--wp--style--block-gap) * 0.4)",
          "name": "X-Small"
        },
        {
          "slug": "30",
          "size": "calc(var(--wp--style--block-gap) * 0.6)",
          "name": "Smaller"
        },
        {
          "slug": "40",
          "size": "calc(var(--wp--style--block-gap) * 0.8)",
          "name": "Small"
        },
        {
          "slug": "50",
          "size": "calc(var(--wp--style--block-gap) * 1)",
          "name": "Medium"
        },
        {
          "slug": "60",
          "size": "calc(var(--wp--style--block-gap) * 1.2)",
          "name": "Large"
        },
        {
          "slug": "70",
          "size": "calc(var(--wp--style--block-gap) * 1.4)",
          "name": "Larger"
        },
        {
          "slug": "80",
          "size": "calc(var(--wp--style--block-gap) * 1.6)",
          "name": "X-Large"
        }
      ],
      "customSpacingSize": false
    },
    "typography": {
      "dropCap": false,
      "customFontSize": false,
      "textTransform": false,
      "fontFamilies": [],
      "letterSpacing": false,
      "fontStyle": false,
      "fontWeight": false,
      "lineHeight": false,
      "textDecoration": false,
      "fontSizes": [
        {
          "slug": "small",
          "name": "Small",
          "size": "var(--font-size-small)"
        },
        {
          "slug": "medium",
          "name": "Medium",
          "size": "var(--font-size-medium)"
        },
        {
          "slug": "large",
          "name": "Large",
          "size": "var(--font-size-large)"
        },
        {
          "slug": "larger",
          "name": "Larger",
          "size": "var(--font-size-larger)"
        }
      ]
    },
    "layout": {
      "contentSize": "var(--content-width)",
      "wideSize": "var(--last-bp)"
    },
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "custom": false,
      "customGradient": false,
      "defaultDuotone": false,
      "palette": [
        {
          "name": "Primary 300",
          "slug": "primary-300",
          "color": "var(--color-primary-300)"
        },
        {
          "name": "Primary 500",
          "slug": "primary-500",
          "color": "var(--color-primary-500)"
        },
        {
          "name": "Primary 700",
          "slug": "primary-700",
          "color": "var(--color-primary-700)"
        },
        {
          "name": "Secondary 300",
          "slug": "secondary-300",
          "color": "var(--color-secondary-300)"
        },
        {
          "name": "Secondary 500",
          "slug": "secondary-500",
          "color": "var(--color-secondary-500)"
        },
        {
          "name": "Secondary 700",
          "slug": "secondary-700",
          "color": "var(--color-secondary-700)"
        },
        {
          "name": "Tertiary 300",
          "slug": "tertiary-300",
          "color": "var(--color-tertiary-300)"
        },
        {
          "name": "Tertiary 500",
          "slug": "tertiary-500",
          "color": "var(--color-tertiary-500)"
        },
        {
          "name": "Tertiary 700",
          "slug": "tertiary-700",
          "color": "var(--color-tertiary-700)"
        },
        {
          "name": "Gray 0",
          "slug": "gray-0",
          "color": "var(--color-gray-0)"
        },
        {
          "name": "Gray 100",
          "slug": "gray-100",
          "color": "var(--color-gray-100)"
        },
        {
          "name": "Gray 300",
          "slug": "gray-300",
          "color": "var(--color-gray-300)"
        },
        {
          "name": "Gray 500",
          "slug": "gray-500",
          "color": "var(--color-gray-500)"
        },
        {
          "name": "Gray 700",
          "slug": "gray-700",
          "color": "var(--color-gray-700)"
        },
        {
          "name": "Gray 900",
          "slug": "gray-900",
          "color": "var(--color-gray-900)"
        },
        {
          "name": "Gray 1000",
          "slug": "gray-1000",
          "color": "var(--color-gray-1000)"
        },
        {
          "name": "System Success",
          "slug": "system-success",
          "color": "var(--color-system-success)"
        },
        {
          "name": "System Warning",
          "slug": "system-warning",
          "color": "var(--color-system-warning)"
        },
        {
          "name": "System Error",
          "slug": "system-error",
          "color": "var(--color-system-error)"
        },
        {
          "name": "System Info",
          "slug": "system-info",
          "color": "var(--color-system-info)"
        }
      ]
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "var(--grid-gutter-height)"
    }
  }
}
